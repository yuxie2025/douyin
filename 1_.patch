+                if (action == null || action.onResult(permissions[n], results[n])) {
+                    iterator.remove();
+                    break;
+                }
+            }
+        }
+        for (int n = 0; n < size; n++) {
+            mPendingRequests.remove(permissions[n]);
+        }
+    }
