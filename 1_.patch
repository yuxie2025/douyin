+
+    private static boolean isJPEG(final byte[] b) {
+        return b.length >= 2
+                && (b[0] == (byte) 0xFF) && (b[1] == (byte) 0xD8);
+    }
+
+    private static boolean isGIF(final byte[] b) {
+        return b.length >= 6
+                && b[0] == 'G' && b[1] == 'I'
+                && b[2] == 'F' && b[3] == '8'
+                && (b[4] == '7' || b[4] == '9') && b[5] == 'a';
