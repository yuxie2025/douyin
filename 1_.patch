+    public static String getImageType(final byte[] bytes) {
+        if (isJPEG(bytes)) return "JPEG";
+        if (isGIF(bytes)) return "GIF";
+        if (isPNG(bytes)) return "PNG";
+        if (isBMP(bytes)) return "BMP";
+        return null;
