+    /**
+     * stack模糊图片
+     *
+     * @param src    源图片
+     * @param radius 模糊半径
+     * @return stack模糊后的图片
+     */
+    public static Bitmap stackBlur(final Bitmap src, final int radius) {
+        return stackBlur(src, radius, false);
+    }
