+        boolean i = false;
+        NetworkInfo localNetworkInfo = ((ConnectivityManager) paramContext
+                .getSystemService(Context.CONNECTIVITY_SERVICE)).getActiveNetworkInfo();
+        if ((localNetworkInfo != null) && (localNetworkInfo.isAvailable()))
+            return true;
+        return false;
